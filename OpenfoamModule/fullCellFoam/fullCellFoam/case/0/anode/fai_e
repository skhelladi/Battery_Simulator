FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/gas";
    object      fai_e;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 0 0 0 0 ];

internalField   uniform   -0.075;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

      anodeCC
    {
        //type            fixedValue;
        //value           uniform 0;
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
    topAndBottom
    {
        type            empty;
    }
    anode_to_seperator
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           $internalField;
        Tnbr            fai_e;
        kappaMethod     lookup;
        kappa           k_fai_e; 
    }

}


