FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/anode";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    anodeCC
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
    topAndBottom
    {
        type            empty;
    }
    anode_to_seperator
    {
        type            calculated;
        value           uniform 0;
    }
}


